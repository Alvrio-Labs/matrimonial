  "main": "index.js",
  "scripts": {
    "production:start": "npx sequelize-cli db:create || npx sequelize-cli db:migrate && nodemon server.js dotenv -e .env.production",
    "development:start": "npx sequelize-cli db:create || npx sequelize-cli db:migrate && nodemon server.js dotenv -e .env.development",
    "test:start": "npx sequelize-cli db:create || npx sequelize-cli db:migrate && nodemon server.js dotenv -e .env.test",
    "development:db:create": "npx sequelize-cli db:create --env development",
    "production:db:create": "npx sequelize-cli db:create --env production",
    "test:db:create": "npx sequelize-cli db:create --env test",
    "lint": "eslint .",
    "development:db:migrate": "npx sequelize-cli db:migrate --env development",
    "production:db:migrate": "npx sequelize-cli db:migrate --env production",
    "test:db:migrate:": "npx sequelize-cli db:migrate --env test",
    "development:db:seed": "npx sequelize-cli db:seed:all --env development",
    "production:db:seed": "npx sequelize-cli db:seed:all --env production",
    "test:db:seed": "npx sequelize-cli db:seed:all --env test"
  },
  "repository": {
